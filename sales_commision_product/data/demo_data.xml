        <!-- Demo Invoice 1: John Smith - Multiple products -->
        <record id="demo_invoice_001" model="account.move">
            <field name="move_type">out_invoice</field>
            <field name="partner_id" ref="partner_customer_acme"/>
            <field name="invoice_user_id" ref="user_salesperson_john"/>
            <field name="invoice_date" eval="(datetime.now() - timedelta(days=15)).strftime('%Y-%m-%d')"/>
            <field name="invoice_line_ids" eval="[
                (0, 0, {
                    'product_id': ref('product_laptop_demo'),
                    'name': 'Demo Laptop Pro 15&quot;',
                    'quantity': 3,
                    'price_unit': 1500.00,
                }),
                (0, 0, {
                    'product_id': ref('product_monitor_demo'),
                    'name': 'Demo 4K Monitor 27&quot;',
                    'quantity': 3,
                    'price_unit': 450.00,
                }),
            ]"/>
        </record>

        <!-- Demo Invoice 2: Sarah Johnson - Smartphones -->
        <record id="demo_invoice_002" model="account.move">
            <field name="move_type">out_invoice</field>
