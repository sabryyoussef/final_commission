<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Demo Products with Commission Rates -->
        <record id="product_laptop_demo" model="product.product">
            <field name="name">Demo Laptop Pro 15"</field>
            <field name="type">consu</field>
            <field name="list_price">1500.00</field>
            <field name="standard_price">900.00</field>
            <field name="commission_rate">8.5</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
        </record>

        <record id="product_smartphone_demo" model="product.product">
            <field name="name">Demo Smartphone X</field>
            <field name="type">consu</field>
            <field name="list_price">899.00</field>
            <field name="standard_price">600.00</field>
            <field name="commission_rate">5.0</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
        </record>

