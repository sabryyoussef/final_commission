        </record>

        <!-- Demo Salespersons (Users) -->
        <record id="user_salesperson_john" model="res.users">
            <field name="name">John Smith (Sales Demo)</field>
            <field name="login">john.smith@demo.com</field>
            <field name="email">john.smith@demo.com</field>
            <field name="groups_id" eval="[(6, 0, [ref('sales_team.group_sale_salesman')])]"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="user_salesperson_sarah" model="res.users">
            <field name="name">Sarah Johnson (Sales Demo)</field>
            <field name="login">sarah.johnson@demo.com</field>
